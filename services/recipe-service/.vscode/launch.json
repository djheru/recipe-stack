{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Debug: task-management",
      "remoteRoot": "/usr/src/app",
      "localRoot": "${workspaceFolder}/nestjs-task-management",
      "protocol": "inspector",
      "port": 9229,
      "restart": true,
      "address": "0.0.0.0",
      "outFiles": ["${workspaceFolder}/nestjs-task-management/dist/**/*.js"],
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true
    }
  ]
}

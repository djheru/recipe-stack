{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "users-service debugger",
      "preLaunchTask": "tsc-watch",
      "protocol": "auto",
      "port": 9229,
      "address": "0.0.0.0",
      "restart": true,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/usr/src/app",
      "outFiles": ["${workspaceFolder}/lib/**/*.js"],
      "skipFiles": ["<node_internals>/**/*.js", "<node_modules>/**/*.js"]
    }
  ]
}
